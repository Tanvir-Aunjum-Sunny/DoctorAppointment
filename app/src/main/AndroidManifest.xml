<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hira.preproject3">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register"
            android:label="Appointment Form"
            android:theme="@style/AppTheme"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".PatientList"
            android:label="Patient List"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".ShowDoctor"
            android:label="Doctor List"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".BasicPres"
            android:label="Basic Health Tips"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Help"
            android:label="Help"
            android:parentActivityName=".MainActivity"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>